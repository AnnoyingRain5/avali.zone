<!DOCTYPE html>
<html lang="en">

<head>
    <link rel="stylesheet" href="/static/css/practice.css">
    <script src="/static/js/subpage-practice.js"></script>
</head>

<body>
<div class="content">
    <div class="question_box_holder">
        <div class="question_box">
            <h1 class="question">{{ prompt }}</h1>
        </div>
    </div>

    <div class="dock">
        <div class="pod">
            <input id="answer">
            <button id="submit"
                    onclick="check_answer(document.getElementById('answer').value, '{{ prompt }}')">Submit
            </button>
        </div>
    </div>
</div>
</body>

</html>